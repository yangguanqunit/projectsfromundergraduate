<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>ldu.se.dhcpserver.HelloWorld</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>long</servlet-name>
        <servlet-class>ldu.se.dhcpserver.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>long</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Dynamic</servlet-name>
        <servlet-class>ldu.se.dhcpserver.DynamicConf</servlet-class>
    </servlet>>
    <servlet-mapping>
        <servlet-name>Dynamic</servlet-name>
        <url-pattern>/dynamic</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Auto</servlet-name>
        <servlet-class>ldu.se.dhcpserver.Auto</servlet-class>
    </servlet>>
    <servlet-mapping>
        <servlet-name>Auto</servlet-name>
        <url-pattern>/auto</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Artificial</servlet-name>
        <servlet-class>ldu.se.dhcpserver.Artificial</servlet-class>
    </servlet>>
    <servlet-mapping>
        <servlet-name>Artificial</servlet-name>
        <url-pattern>/artificial</url-pattern>
    </servlet-mapping>
    
</web-app>
